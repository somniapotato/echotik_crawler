CREATE DATABASE videos;

CREATE TABLE echotik_videos (
    video_id BIGINT PRIMARY KEY,
    title VARCHAR(255),
    video_url VARCHAR(255),
    share_url VARCHAR(255),
    sales INT,
    views INT,
    er_ratio FLOAT,
    comments INT,
    shares INT,
    influencer_id VARCHAR(255),
    influencer_name VARCHAR(255),
    product_info TEXT,
    ctime INT,
    mtime INT
);